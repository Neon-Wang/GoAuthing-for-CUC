[Unit]
Description=Authenticating utility for net.cuc.edu.cn
StartLimitIntervalSec=0

[Service]
ExecStartPre=-/usr/local/bin/auth-cuc -c /etc/goauthing.json -D deauth
ExecStartPre=-/usr/local/bin/auth-cuc -c /etc/goauthing.json -D auth
ExecStart=/usr/local/bin/auth-cuc -c /etc/goauthing.json online
User=nobody
Restart=always
RestartSec=5

[Install]
WantedBy = multi-user.target
